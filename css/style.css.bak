/* SlideShow */

.mySlides {display: none;}
img {vertical-align: middle;

height:320px;
}

/* Slideshow container */


/* Caption text */
.text {
   color: #f2f2f2;
  font-size:30px;
  padding: 12px 12px;
  position:absolute;
  top: 420px;
/* font-family:var(--estiloTextTitulos); */
    /* bottom: 10px; */
  width: 97%;
  text-align:center;
}



/* Next & previous buttons */
.prev, .next {
  cursor: pointer;
  position: absolute;
  top: 50%;
  width: auto;
  padding: 16px;
  margin-top: -50px;
  margin:30px;
 color:#f2f2f2; 
  font-weight: bold;
  font-size: 38px;
  transition: 0.6s ease;
  user-select: none;
  background:rgba(89, 89, 89,0.3);
   border-radius: 70px;
}

/* Position the "next button" to the right */
.next {
  right: 0;
  border-radius: 70px;
  
}

/* Fading animation */
.fade {
  -webkit-animation-name: fade;
  -webkit-animation-duration: 1.5s;
  animation-name: fade;
  animation-duration: 1.5s;
}

@-webkit-keyframes fade {
  from {opacity: .4} 
  to {opacity: 1}
}

@keyframes fade {
  from {opacity: .4} 
  to {opacity: 1}
}

/* On smaller screens, decrease text size */
@media only screen and (max-width: 300px) {
  .text {font-size: 11px}
}
/*SlideShow*/

